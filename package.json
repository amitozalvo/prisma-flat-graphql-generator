{
  "name": "prisma-flat-graphql-generator",
  "description": "Prisma GraphQL Generator (flat, will not introduce performance overhead). Inspired by pal.js",
  "version": "0.2.0",
  "bin": {
    "prisma-flat-graphql-generator": "dist/generator.js"
  },
  "module": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:setup": "cd test && DATABASE_URL=file:./test.db npx prisma generate && DATABASE_URL=file:./test.db npx ts-node setup.ts",
    "test:server": "cd test && npx ts-node server.ts"
  },
  "contributors": [
    {
      "name": "Amit Ozalvo"
    }
  ],
  "keywords": [
    "prisma",
    "graphql",
    "gqp",
    "flat",
    "generator"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amitozalvo/prisma-flat-graphql-generator.git"
  },
  "bugs": {
    "url": "https://github.com/amitozalvo/prisma-flat-graphql-generator/issues"
  },
  "devDependencies": {
    "@apollo/server": "^5.2.0",
    "@prisma/adapter-better-sqlite3": "^7.1.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.0.0",
    "@types/pluralize": "^0.0.33",
    "@vitest/ui": "^4.0.15",
    "better-sqlite3": "^12.5.0",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "happy-dom": "^20.0.11",
    "prisma": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@prisma/client": "^7.1.0",
    "@prisma/generator-helper": "^7.1.0",
    "@prisma/internals": "^7.1.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.1.1"
  }
}
